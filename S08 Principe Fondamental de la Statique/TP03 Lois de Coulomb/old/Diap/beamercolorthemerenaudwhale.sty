\ProvidesPackageRCS $Header: /cvsroot/latex-beamer/latex-beamer/themes/color/beamercolorthemerenaudwhale.sty,v 1.4 2007/01/28 20:48:24 tantau Exp $


\mode<presentation>

%Bleu très clair
\definecolor{bleb}{RGB}{228,237,250}
%Bleu moyen
\definecolor{blec}{RGB}{151,177,214}
%Bleu foncé
\definecolor{blef}{RGB}{82,131,190}
%Rouge
\definecolor{orgr}{RGB}{211,59,56}
%Vert
\definecolor{verr}{RGB}{51,204,51}
%Violet
\definecolor{vior}{RGB}{204,0,204}


\setbeamercolor*{normal text}{fg=blef}
\setbeamercolor*{alerted text}{fg=blef}
\setbeamercolor*{example text}{fg=blef} 
\setbeamercolor*{item}{fg=orgr}
\setbeamercolor*{itemize subsubitem}{fg=blec}
\setbeamercolor*{itemize item}{fg=blef}
\setbeamercolor*{itemize subitem}{fg=blec}
\setbeamercolor*{section in toc}{fg=blef}
\setbeamercolor*{subsection in head/foot}{fg=blef}
\setbeamercolor*{title}{fg=orgr}
\setbeamercolor*{frametitle}{fg=orgr,bg=black!0}

\setbeamercolor*{palette primary}{fg=bleb ,bg=blef}
%\setbeamercolor*{palette primary}{fg=blef!60!blec,bg=blec!85!bleb}
\setbeamercolor*{palette secondary}{fg=white,bg=red}
\setbeamercolor*{palette tertiary}{fg=white,bg=blec}
\setbeamercolor*{palette quaternary}{fg=white,bg=yellow}

\setbeamercolor*{sidebar}{use=structure,bg=blef!30!white}
  
\setbeamercolor*{palette sidebar primary}{fg=pink}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=pink}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{fg=orgr,bg=white}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
